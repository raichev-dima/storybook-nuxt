{
  "name": "storybook-nuxt",
  "version": "0.1.9-alpha.0",
  "description": "Storybook init CLI for Nuxt ",
  "author": "Chakas@",
  "license": "ISC",
  "keywords": [
    "storybook",
    "cli",
    "nuxt"
  ],
  "bin": {
    "storybook-nuxt": "./index.mjs"
  },
  "files": [
    ".storybook/**/*",
    "dist",
    "*.d.ts",
    "*.cjs",
    "*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev:prepare": "stub && build && test",
    "prepack": "unbuild",
    "dev": "cd playground &&  node ../index.mjs init --enable-module ",
    "test": "cd playground &&  node ../index.mjs init ",
    "release": "pnpm changelogen --release --push && pnpm publish"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^11.0.0",
    "consola": "^3.2.3",
    "diff": "^5.1.0",
    "execa": "^7.2.0",
    "global-dirs": "^3.0.1",
    "magicast": "^0.2.10",
    "pathe": "^1.1.1",
    "picocolors": "^1.0.0",
    "pkg-types": "^1.0.3",
    "prompts": "^2.4.2",
    "rc9": "^2.1.1",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/diff": "^5.0.3",
    "@types/prompts": "^2.4.4",
    "unbuild": "^1.2.1"
  }
}
