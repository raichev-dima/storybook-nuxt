{
  "name": "@storybook-vue/nuxt",
  "version": "0.0.3",
  "packageManager": "pnpm@7.23.0",
  "description": "Storybook for Nuxt and Vite: Develop Vue3 components in isolation with Hot Reloading.",
  "license": "MIT",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook-vue"
  },
  "homepage": "https://github.com/storybook-vue/storybook-nuxt",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybook-vue/storybook-nuxt.git"
  },
  "bugs": {
    "url": "https://github.com/storybookjs/storybook/issues"
  },
  "keywords": [
    "storybook",
    "nuxt",
    "vite",
    "vue3"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.cjs"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "import": "./dist/preview.mjs",
      "require": "./dist/preview.cjs"
    },
    "./preview.js": {
      "types": "./dist/preview.d.ts",
      "import": "./dist/preview.mjs",
      "require": "./dist/preview.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "dist/preset.js",
    "template/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "build:watch": "unbuild --stub",
    "stub": "pnpm run build --stub",
    "test": "vitest run",
    "dev": "pnpm build && pnpm -C playground storybook dev",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "nuxt": "^3.6.5",
    "vite": "*",
    "vue": "^3.2.31"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^0.7.6",
    "@nuxt/kit": "^3.7.0",
    "@nuxt/schema": "^3.7.0",
    "@nuxt/vite-builder": "^3.7.0",
    "@storybook/builder-vite": "7.4.0-alpha.2",
    "@storybook/types": "7.4.0-alpha.2",
    "@storybook/vue3": "npm:@storybook-vue/vue3@7.4.0-alpha.2.1",
    "@storybook/vue3-vite": "npm:@storybook-vue/vue3-vite@7.4.0-alpha.2",
    "autoprefixer": "^10.4.15",
    "nuxt": "^3.7.0",
    "postcss": "^8.4.28",
    "postcss-import": "^15.1.0",
    "postcss-url": "^10.1.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/preset.ts",
      "./src/preview.ts"
    ],
    "platform": "node"
  }
}
